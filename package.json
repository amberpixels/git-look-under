{
  "name": "gitjump",
  "description": "GitHub fuzzy finder browser extension",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--no-webstorage' wxt",
    "dev:firefox": "NODE_OPTIONS='--no-webstorage' wxt -b firefox",
    "build": "NODE_OPTIONS='--no-webstorage' wxt build",
    "build:firefox": "NODE_OPTIONS='--no-webstorage' wxt build -b firefox",
    "zip": "NODE_OPTIONS='--no-webstorage' wxt zip",
    "zip:firefox": "NODE_OPTIONS='--no-webstorage' wxt zip -b firefox",
    "compile": "NODE_OPTIONS='--no-webstorage' vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "postinstall": "NODE_OPTIONS='--no-webstorage' wxt prepare"
  },
  "dependencies": {
    "vue": "^3.5.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1",
    "@wxt-dev/module-vue": "^1.0.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.46.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.6",
    "wxt": "^0.20.6"
  }
}
